{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "quantidade": {
      "type": "integer"
    },
    "usuarios": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          },
          "administrador": {
            "type": "string",
            "enum": ["true", "false"]
          },
          "nome": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "_id": {
            "type": "string"
          }
        },
        "required": ["password", "administrador", "nome", "email", "_id"]
      }
    }
  }
}